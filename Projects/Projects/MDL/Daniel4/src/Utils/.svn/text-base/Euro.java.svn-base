package Utils;

public class Euro implements MoneyCurrency{

	//	 Cette classe représente l'indicateur monétaire euro...
	
	
	private float amount;
	
	public Euro(float amount)
	{
		this.amount = amount;
	}
	
	public String getAmount() {
		// TODO Auto-generated method stub
		return new String(new Float(this.amount).toString());
	}

	
	
	
}
